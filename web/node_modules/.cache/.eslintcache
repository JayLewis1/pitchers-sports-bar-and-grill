         <li>\n              <span className=\"img-wrapper\">\n                <img src=\"/assets/whatson/wo1@2x.jpg\" alt=\"Whats on image\"/>\n                <span className=\"date-con\">\n                <p>Closed</p>\n                </span>\n              </span>\n              <span className=\"text-wrapper\">\n                <p className=\"whatson-title\">We are currently closed</p>\n                <p className=\"whatson-subtitle\">Due to Covid-19 we are closed but we'll let you know when we are open again.</p>\n                <button>\n                  <span className=\"btn-bg\"></span>\n                  <p>Get in contact</p></button>\n              </span>\n            </li>\n        </ul>\n    </div>\n  )\n}\n\nexport default WhatsOn;","/Users/jay/Desktop/Current Projects/pitchers-site/web/src/components/home/GalleryPopup.tsx",["99"],"import React, { useState } from 'react'\n//Redux\nimport { connect, ConnectedProps } from \"react-redux\";\n\ninterface ComponentProps {\n  gallery: {\n    toggle: boolean\n  }\n}\n\nconst mapState = (state:ComponentProps ) => ({\n  gallery: state.gallery\n})\n\nconst mapDispatch = {\n  toggleGallery : (payload: boolean) => ({type: \"TOGGLE_GALLERY\", payload: payload})\n}\n\nconst connector = connect(mapState, mapDispatch)\n\ntype PropsFromRedux = ConnectedProps<typeof connector>\ntype Props = PropsFromRedux & {\n  index : number\n}\n\nconst GalleryPopup = ({index, toggleGallery}:Props ) => {\n  const [imgIndex, setImgIndex] = useState(index);\n\n  const changeImgLeft = () => {\n    let newIndex = imgIndex - 1;\n    if(newIndex < 1) {\n      newIndex = 5;\n      setImgIndex(newIndex);\n    } else {\n      setImgIndex(newIndex);\n    }\n  }\n  const changeImgRight = () => {\n    let newIndex = imgIndex + 1;\n    if(newIndex > 5) {\n      newIndex = 1;\n      setImgIndex(newIndex);\n    } else {\n      setImgIndex(newIndex);\n    }\n  }\n  const exitGallery = () => {\n    console.log(\"exit gallery\");\n    toggleGallery(false)\n  }\n\n  return (\n    <div className=\"gallery-popup\">\n        <div className=\"img-wrapper\">\n            <img src={`/assets/photos/g${imgIndex}@2x.jpg`} alt=\"Gallery photo\"/>\n            <span className=\"button-wrapper\">\n              <button onClick={() => changeImgLeft()}>left</button>\n              <button onClick={() => exitGallery()} className=\"exit-btn\">exit gallery</button>\n              <button onClick={() => changeImgRight()}>right</button>\n            </span>\n        </div>\n    </div>\n  )\n}\nexport default connector(GalleryPopup);","/Users/jay/Desktop/Current Projects/pitchers-site/web/src/redux/store.ts",["100"],"import { createElement } from \"react\";\nimport { createStore, applyMiddleware, compose } from \"redux\";\nimport { composeWithDevTools } from \"redux-devtools-extension\";\nimport thunk from \"redux-thunk\";\nimport { rootReducer } from \"./reducers\";\n\nconst initialState = {};\n\nconst middleware = [thunk]\n\nexport const store = createStore(rootReducer, initialState, compose(composeWithDevTools(applyMiddleware(...middleware))))\n\nexport default store;","/Users/jay/Desktop/Current Projects/pitchers-site/web/src/redux/reducers/index.ts",[],"/Users/jay/Desktop/Current Projects/pitchers-site/web/src/redux/reducers/gallery.ts",[],"/Users/jay/Desktop/Current Projects/pitchers-site/web/src/redux/actions/types.ts",[],{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","severity":1,"message":"106","line":4,"column":8,"nodeType":"107","messageId":"108","endLine":4,"endColumn":11},{"ruleId":"105","severity":1,"message":"109","line":1,"column":27,"nodeType":"107","messageId":"108","endLine":1,"endColumn":35},{"ruleId":"101","replacedBy":"110"},{"ruleId":"103","replacedBy":"111"},{"ruleId":"112","severity":1,"message":"113","line":8,"column":9,"nodeType":"114","endLine":8,"endColumn":291},{"ruleId":"115","severity":1,"message":"116","line":13,"column":19,"nodeType":"114","endLine":13,"en